class Solution {
    public void rev(int[] num ,int i,int j){
        while(i<j){
            int temp = num[i];
            num[i]=num[j];
            num[j]=temp;
            i++;
            j--;
        }
    }
    public void swap(int[] num,int i,int j){
        int temp =num[i];
        num[i]=num[j];
        num[j]= temp;
    }
    public void nextPermutation(int[] nums) {
        int n = nums.length;
        int inx=-1;
        for(int i=n-2;i>=0;i--){
            if(nums[i]<nums[i+1]){
                inx=i;
                break;
            }
        }
        if(inx==-1){
            rev(nums,0,n-1);
            return ;
        }

        for(int i=n-1;i>=inx;i--){
            if(nums[i]>nums[inx]){
                swap(nums,i,inx);
                break;
            }
        }
        rev(nums,inx+1,n-1);
    
        return ;
    }
}
