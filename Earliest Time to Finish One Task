class Solution {
    public int earliestTime(int[][] tasks) {
        int n =tasks.length;
        int result =Integer.MAX_VALUE;
        for(int i=0;i<n;i++){
            int sum=0;
            sum +=tasks[i][0];
            sum +=tasks[i][1];
            if(result>sum){
                result =sum;
            }
        }
        return result;
    }
}
