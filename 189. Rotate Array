class Solution {
    public void rotate(int[] nums, int k) {
        int n= nums.length;
        k=k%n;
        int[] temp = new int[k];
        for(int i=n-k;i<n;i++){
            temp[i-n+k]=nums[i];
        }
        int j=1;
        for(int i=n-k-1;i>=0;i--){
            nums[n-j]=nums[i];
            j++;
        }
        for(int i=0;i<k;i++){
            nums[i]=temp[i];
        }

    }
}
