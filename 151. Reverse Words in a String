class Solution {
    public String reverseWords(String s) {
        StringBuilder sb = new StringBuilder();
        StringBuilder result = new StringBuilder();

        for(char ch : s.toCharArray()){
            if(ch == ' '){

                result.insert(0,sb);
                if(sb.length()>0){
                    result.insert(0," ");
                    sb.setLength(0);
                }
                

                
            }else{
                sb.append(ch);
            }
        }
        result.insert(0,sb);

        return result.toString().trim();
    }
}
