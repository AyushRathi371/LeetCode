class Solution {
    public int[] searchRange(int[] nums, int target) {
        int n= nums.length;
        int ans=-1;
        int ans1=-1;
        int low =0;
        int high = n-1;
        int mid=0;
        while(low<=high){
            mid = (low+high)/2;
            if(nums[mid]==target){
                ans= mid;
                ans1=mid;
                break;
            }else if(nums[mid]>target){
                high=mid-1;
            }else{
                low=mid+1;
            }
        }
        if(ans == mid){
            int i=mid;
            while(i>=0 &&nums[i]==target){
                ans = i;
                i--;
            }i=mid;
            while(i<n &&nums[i]==target){
                ans1= i;
                i++;
            }
        }
        return new int[] {ans,ans1};
    }
}
