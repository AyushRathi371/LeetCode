class Solution {
    public int[] intersect(int[] nums1, int[] nums2) {
        int i=0;
        int j=0;
        Arrays.sort(nums1);
        Arrays.sort(nums2);

       int  n=nums1.length;
        int m=nums2.length;
        List<Integer> list= new ArrayList<>();        
       while(i<n && j<m){
        if(nums1[i] <nums2[j]){
            i++;
        }
        else if(nums1[i]>nums2[j]){
            j++;
        }
        else{
            list.add(nums1[i]);
            i++;
            j++;
        }
       }
        int [] result= new int[list.size()];
        for(int l=0;l<list.size();l++){
            result[l] =list.get(l);
       }
       return result;
    }
}
