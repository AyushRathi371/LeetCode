class Solution {
    public int findNumbers(int[] nums) {
        int i,digit,num=0;
    for(i=0;i< nums.length;i++){
        int count=0;
        digit = nums[i];
        while(digit != 0){
            digit = digit/10;
            count++;
        }
        if(count%2==0){
            num++;
        }
       
    }
    return num;
    }
}
