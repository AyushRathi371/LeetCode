class Solution {
    public boolean isAnagram(String s, String t) {
        int n = s.length();
        if(n!= t.length())return false;

        char[] first = s.toCharArray();
        char[] second = t.toCharArray();
        for(int i=0;i<n;i++){
            boolean found = false;
            for(int j=0;j<n;j++){
                if(first[i]==second[j]){
                    second[j] = '/';
                    found =true;
                    break;
                }
            }
            if(!found) return false;
        }
        return true;
    }
}
