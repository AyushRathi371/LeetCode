class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int n=nums.length;
        int j=0;
        int count =0;
        for(int i=0;i<n;i++){
            if(nums[i]==1){
                count++;
            }if(nums[i]==0  || i==nums.length-1){
                if(count>=j){
                    j=count;
                    count=0;
                }
                else if(count>0){
                    count=0;
                }
            }
        }return j;
    }
}
