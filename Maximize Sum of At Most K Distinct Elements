class Solution {
    public int[] maxKDistinct(int[] nums, int k) {
        int n = nums.length;
        Arrays.sort(nums);
        int[] sum = new int[k];
        int cnt=0;
        for(int i=n-1;i>=0;i--){
            if(i<n-1 && nums[i]==nums[i+1]) continue;

            sum[cnt++]=nums[i];
            if(cnt==k) break;
            
        }
        int[] result = new int[cnt];
        for(int i=0;i<cnt;i++){
            result[i]=sum[i];
        }
        return result;
    }
}
