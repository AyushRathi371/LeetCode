class Solution {
    public String largestOddNumber(String num) {
        int n = num.length();
        int cnt =0;
        int result =0;
        StringBuilder sb = new StringBuilder();
        for(int i=n-1;i>=0;i--){
            int x = (int) num.charAt(i);
            if(x%2==1){
                cnt++;
                
                for(int j=0;j<=i;j++){
                    sb.append(num.charAt(j));
                }
                break;
            }
        }
        if(cnt ==1){
            return sb.toString();
        }
        return "";
    }
}
