class Solution {
    public int findMin(int[] nums) {
        int min = Integer.MAX_VALUE;
        int n = nums.length;
        int low = 0;
        int high = n-1;
        while(low<=high){
            int mid= (low+high)/2;
            if(nums[mid]<min){
                min = nums[mid];
            }

            if(nums[low]<=nums[mid]){
                if(nums[low]<nums[high]){
                    high = mid-1;
                }else{
                    low = mid+1;
                }
            }else{
                if(nums[mid]<nums[high]){
                    high = mid-1;
                }else{
                    low = mid+1;
                }
            }
        }
        return min;

    }
}
